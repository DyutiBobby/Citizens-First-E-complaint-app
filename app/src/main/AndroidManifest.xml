<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.complaintt">

    <uses-permission android:name="android.permission.SEND_SMS" />
    <uses-permission android:name="android.permission.RECEIVE_SMS" />

    <uses-feature
        android:name="android.hardware.telephony"
        android:required="true" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ComplaintT">
        <activity android:name=".Notification2"></activity>
        <activity android:name=".Notification1" />
        <activity android:name=".Profile" />
        <activity android:name=".login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AdminActivity" />
        <activity android:name=".MainActivity" />
        <activity android:name=".Register" />
        <activity android:name=".Notification" />
        <activity android:name=".Submit" />
        <activity android:name=".Harassment" />
        <activity android:name=".Accident" />
        <activity android:name=".Theft" />
    </application>

</manifest>